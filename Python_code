<div class="highlight hl-ipython3"><pre><span></span><span class="n">cardiac_segmentation</span> <span class="o">=</span> <span class="p">[</span>
    <span class="p">(</span><span class="n">num_cardiac_clm_by_yr_npi_df</span><span class="p">[</span><span class="s1">'claim_id'</span><span class="p">]</span> <span class="o">==</span> <span class="mi">1</span><span class="p">),</span>
    <span class="p">(</span><span class="n">num_cardiac_clm_by_yr_npi_df</span><span class="p">[</span><span class="s1">'claim_id'</span><span class="p">]</span> <span class="o">&gt;=</span> <span class="mi">2</span><span class="p">)</span> <span class="o">&amp;</span> <span class="p">(</span><span class="n">num_cardiac_clm_by_yr_npi_df</span><span class="p">[</span><span class="s1">'claim_id'</span><span class="p">]</span> <span class="o">&lt;</span> <span class="mi">4</span><span class="p">),</span>
    <span class="p">(</span><span class="n">num_cardiac_clm_by_yr_npi_df</span><span class="p">[</span><span class="s1">'claim_id'</span><span class="p">]</span> <span class="o">&gt;=</span> <span class="mi">5</span><span class="p">)</span> <span class="o">&amp;</span> <span class="p">(</span><span class="n">num_cardiac_clm_by_yr_npi_df</span><span class="p">[</span><span class="s1">'claim_id'</span><span class="p">]</span> <span class="o">&lt;</span> <span class="mi">9</span><span class="p">),</span>

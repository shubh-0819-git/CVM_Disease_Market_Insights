<span class="p">]</span>

<span class="n">letters</span> <span class="o">=</span> <span class="p">[</span><span class="s1">'Disease Aware'</span><span class="p">,</span> <span class="s1">'Trialists'</span><span class="p">,</span> <span class="s1">'Rising Stars'</span><span class="p">,</span> <span class="s1">'High-Volumne Prescribers'</span><span class="p">]</span>

